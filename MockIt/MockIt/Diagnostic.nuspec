<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>MockIt</id>
    <version>1.0.0.10</version>
    <title>MockIt</title>
    <authors>Yevhen Cherkes</authors>
    <owners>Yevhen Cherkes</owners>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/ycherkes/MockIt</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A simple Visual Studio 2015 extension to automate of creating and configuring mocks in the unit tests.
    Also available via Visual Studio gallery https://visualstudiogallery.msdn.microsoft.com/713cf20e-e553-4bc9-ac02-477c750fad61</description>
    <copyright>Yevhen Cherkes</copyright>
    <tags>Code generation, NUnit, MSTest, Mock, Auto Mocking</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="*.dll" target="tools\analyzers\" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>